<tool id='fasttext-tester' version='1.0.0'>
  <description>A fasttext tester for all three types of models.</description>
  <command>python3 $__tool_directory__/fasttext-tester.py -t $test -m $model</command>
  <inputs>
    <param name='test' type='data' format='fasttext'/>
    <param name='model' type='data' format='model'/>
  </inputs>
  <outputs>
    <data name='output' format='text' />
  </outputs>
</tool>